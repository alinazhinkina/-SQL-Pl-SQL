OPTIONS (SKIP=1)
LOAD DATA
CHARACTERSET UTF8
INFILE 'fact_oper.csv'
BADFILE 'fact_oper.bad'
DISCARDFILE 'fact_oper.dsc'
TRUNCATE
INTO TABLE creditor.fact_oper
FIELDS TERMINATED BY ';'
OPTIONALLY ENCLOSED BY '"'
TRAILING NULLCOLS
( 
  COLLECTION_ID,
  F_DATE DATE "dd.mm.yyyy",
  F_SUMMA "TO_NUMBER(REPLACE(:F_SUMMA, ',','.'))",
  TYPE_OPER
)