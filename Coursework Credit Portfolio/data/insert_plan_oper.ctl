OPTIONS (SKIP=1)
LOAD DATA
CHARACTERSET UTF8
INFILE 'plan_oper.csv'
BADFILE 'plan_oper.bad'
DISCARDFILE 'plan_oper.dsc'
TRUNCATE
INTO TABLE creditor.plan_oper
FIELDS TERMINATED BY ';'
